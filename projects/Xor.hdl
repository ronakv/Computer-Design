/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = b, out = notB);
    Not(in = a, out = notA);
    And(a = b, b = notA, out = bAndNotA);
    And(a = notB, b = a, out = aAndNotB);
    Or(a = aAndNotB, b = bAndNotA, out = out);
}